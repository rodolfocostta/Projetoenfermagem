@model EnfermariaSenac.Models.Enfermagem

@{
    ViewData["Title"] = "Editar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Editar</h1>

<h4>Enfermagem</h4>
<hr />
<div class="row">
    <div class="col-md-4 mx-auto">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ID_Enfermagem" />
            <div class="form-group">
                <label asp-for="Nome_Enfermagem" class="control-label"></label>
                <input asp-for="Nome_Enfermagem" class="form-control" />
                <span asp-validation-for="Nome_Enfermagem" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Coren" class="control-label"></label>
                <input asp-for="Coren" class="form-control" />
                <span asp-validation-for="Coren" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Voltar para lista</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
